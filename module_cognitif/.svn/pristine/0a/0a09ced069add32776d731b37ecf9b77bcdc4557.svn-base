   <variables> 
      <var_folder name="Home"> 
         <var name="address" type="TEXT"/>  
         <var name="france_or_abroad" type="TEXT"/>  
         <var name="country_or_region" type="TEXT"/>  
         <var name="sentence" type="TEXT"/>  
         <var name="out_of_scope_count" type="NUMBER" initValue="0"/>  
