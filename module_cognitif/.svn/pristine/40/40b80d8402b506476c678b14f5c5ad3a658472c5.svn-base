<?xml version="1.0" encoding="UTF-8"?>

<dialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd">  
   <flow> 
      <folder label="Main"> 
         <output> 
            <prompt selectionType="RANDOM"> 
               <item>Avez-vous un projet spécifique ou avez-vous un besoin de trésorerie ? [action->setVariable(client_score)]</item> 
            </prompt>  
            <action varName="current_state" operator="SET_TO">project_type</action>  
            <getUserInput> 
               <search ref="project_type"> 
                  <classifier policy="SEARCHMODE_CLASSIFIER" threshold="0.75" exactMatchFirst="true" fuzzyThreshold="0.1" trainOnInput="true" trainOnOutput="false"> 
                     <customParams>nlcmodelid=2373f5x67-nlc-792</customParams> 
                  </classifier> 
               </search>  
               <search ref="questions"> 
                  <classifier policy="SEARCHMODE_CLASSIFIER" threshold="0.70" exactMatchFirst="true" fuzzyThreshold="0.1" trainOnInput="true" trainOnOutput="false"> 
                     <customParams>nlcmodelid=2373f5x67-nlc-792</customParams> 
                  </classifier> 
               </search>  
               <default> 
                  <if> 
                     <cond varName="out_of_scope_count" operator="GREATER_THEN">1</cond>  
                     <goto ref="get_sentence"/> 
                  </if>  
                  <output> 
                     <prompt> 
                        <item>Je ne comprends pas ce que vous dites. Avez-vous un projet spécifique ou avez-vous un besoin de trésorerie ?</item> 
                     </prompt>  
                     <action varName="out_of_scope_count" operator="INCREMENT_BY">1</action>  
                     <getUserInput> 
                        <search ref="project_type"> 
                           <classifier policy="SEARCHMODE_CLASSIFIER" threshold="0.75" exactMatchFirst="true" fuzzyThreshold="0.1" trainOnInput="true" trainOnOutput="false"> 
                              <customParams>nlcmodelid=2373f5x67-nlc-792</customParams> 
                           </classifier> 
                        </search>  
                        <search ref="questions"> 
                           <classifier policy="SEARCHMODE_CLASSIFIER" threshold="0.70" exactMatchFirst="true" fuzzyThreshold="0.1" trainOnInput="true" trainOnOutput="false"> 
                              <customParams>nlcmodelid=2373f5x67-nlc-792</customParams> 
                           </classifier> 
                        </search> 
                     </getUserInput> 
                  </output> 
               </default> 
            </getUserInput> 
         </output> 
      </folder>  
      <folder label="Library"> 
         <folder label="Live Content" id="liveContent"> 
            <folder label="Project types" id="project_type"> 
               <action varName="current_state" operator="SET_TO">project_type</action>  
               <action varName="out_of_scope_count" operator="SET_TO">0</action>  
               <input> 
                  <grammar> 
                     <item>car_acquisition</item>  
                     <item>$ acheter une voiture</item>  
                     <item>$ achat de voiture</item>
                     <item>$ louer une voiture</item>
                  </grammar>  
                  <action varName="hire_specific_project" operator="SET_TO">car</action>  
                  <action varName="is_specific_project" operator="SET_TO_YES"/>  
                  <if matchType="ALL"> 
                     <cond varName="asked_for_500" operator="EQUAL_TO_NO"/>  
                     <cond varName="client_score" operator="GREATER_THEN">10</cond>  
                     <goto ref="need_500"/> 
                  </if>  
                  <goto ref="three_types_loan"/> 
               </input>  
               <input> 
                  <grammar> 
                     <item>$ acheter une moto</item>  
                     <item>motorcycle</item> 
                  </grammar>  
                  <action varName="hire_specific_project" operator="SET_TO">motorcycle</action>  
                  <action varName="is_specific_project" operator="SET_TO_YES"/>  
                  <if matchType="ALL"> 
                     <cond varName="asked_for_500" operator="EQUAL_TO_NO"/>  
                     <cond varName="client_score" operator="GREATER_THEN">10</cond>  
                     <goto ref="need_500"/> 
                  </if>  
                  <goto ref="three_types_loan"/> 
               </input>  
