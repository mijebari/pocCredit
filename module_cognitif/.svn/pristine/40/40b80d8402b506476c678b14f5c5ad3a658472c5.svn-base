                     <cond varName="client_score" operator="GREATER_THEN">10</cond>  
                     <goto ref="need_500"/> 
                  </if>  
                  <goto ref="two_types_loan"/> 
               </input>  
               <input> 
                  <grammar> 
                     <item>student_loan</item>  
                     <item>$ prêt étudiant</item> 
