                     <cond varName="asked_for_500" operator="EQUAL_TO_NO"/>  
                     <cond varName="client_score" operator="GREATER_THEN">10</cond>  
                     <goto ref="need_500"/> 
                  </if>  
                  <goto ref="two_types_loan"/> 
               </input>  
               <input> 
                  <grammar> 
                     <item>is_specific_project</item>  
                     <item>$ projet spécifique</item> 
                  </grammar>  
                  <action varName="is_specific_project" operator="SET_TO_YES"/>  
                  <if matchType="ALL"> 
                     <cond varName="asked_for_500" operator="EQUAL_TO_NO"/>  
                     <cond varName="client_score" operator="GREATER_THEN">10</cond>  
                     <goto ref="need_500"/> 
                  </if>  
                  <output> 
                     <prompt selectionType="RANDOM"> 
                        <item>Quel projet spécifique avez-vous ?</item>  
