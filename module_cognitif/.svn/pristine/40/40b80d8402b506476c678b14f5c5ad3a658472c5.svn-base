                     <cond varName="asked_for_500" operator="EQUAL_TO_NO"/>  
                     <cond varName="client_score" operator="GREATER_THEN">10</cond>  
                     <goto ref="need_500"/> 
                  </if>  
                  <goto ref="two_types_loan"/> 
               </input>  
               <input> 
                  <grammar> 
                     <item>cash_resources</item>  
                     <item>$ besoin de trésorerie</item> 
                  </grammar>  
