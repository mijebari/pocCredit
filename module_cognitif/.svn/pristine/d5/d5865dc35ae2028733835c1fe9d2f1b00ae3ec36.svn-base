<?xml version="1.0" encoding="UTF-8"?>

<dialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd">  
   <flow> 
      <folder label="Main"> 
         <output> 
            <prompt selectionType="RANDOM"> 
               <item>Bonjour [firstname]! En quoi puis-je vous aider ?</item> 
            </prompt>  
            <getUserInput> 
               <search ref="call_topic"> 
                  <classifier policy="SEARCHMODE_CLASSIFIER" threshold="0.85" exactMatchFirst="true" fuzzyThreshold="0.1" trainOnInput="true" trainOnOutput="false"> 
                     <customParams>nlcmodelid=2373f5x67-nlc-848</customParams> 
                  </classifier> 
               </search>  
               <search ref="greetings"/>  
               <default> 
                  <output> 
                     <prompt selectionType="RANDOM"> 
                        <item>Je suis désolé, je n'ai pas compris votre question.</item>  
                        <item>Je n'ai pas compris ce que vous avez dit.</item>
                        <item>Je ne vous suis pas.</item>
                     </prompt>  
                     <action varName="Out-of-Scope_Count" operator="INCREMENT_BY">1</action>  
                     <if> 
                        <cond varName="Out-of-Scope_Count" operator="GREATER_THEN">2</cond>  
                        <goto ref="out_of_scope"/> 
                     </if>  
                     <goto ref="help_client"/> 
                  </output> 
               </default> 
            </getUserInput> 
         </output> 
      </folder>  
      <folder label="Library"> 
         <folder label="Live Content" id="liveContent"> 
            <folder label="Dialog" id="dialog"> 
               <output id="help_client"> 
                  <prompt selectionType="RANDOM"> 
                     <item>En quoi puis-je vous aider ?</item>  
                     <item>Que puis-je faire pour vous ?</item>
                  </prompt>  
                  <getUserInput> 
                     <search ref="dialog"></search> 
                  </getUserInput> 
               </output>  
               <output id="out_of_scope"> 
                  <prompt> 
                     <item>Voulez-vous raccrocher ou parler avec un conseiller ?</item> 
                  </prompt>  
                  <action varName="Out-of-Scope_Count" operator="SET_TO">0</action>  
                  <getUserInput> 
                     <search ref="out_of_scope_choice"></search> 
                  </getUserInput> 
               </output>  
               <folder label="Relevant call topics" id="call_topic"> 
